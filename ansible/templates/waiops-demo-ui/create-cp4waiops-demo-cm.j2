apiVersion: v1
kind: ServiceAccount
metadata:
  name: demo-ui-admin
  namespace: {{ WAIOPS_NAMESPACE }}
---
kind: ClusterRoleBinding
apiVersion: rbac.authorization.k8s.io/v1
metadata:
  name: demo-ui-admin-crb
subjects:
  - kind: ServiceAccount
    name: demo-ui-admin
    namespace: {{ WAIOPS_NAMESPACE }}
roleRef:
  apiGroup: rbac.authorization.k8s.io
  kind: ClusterRole
  name: cluster-admin
---
apiVersion: batch/v1
kind: Job
metadata:
  name: demo-ui-create-config
  namespace: {{ WAIOPS_NAMESPACE }}
spec:
  template:
    spec:
      containers:
        - name: config
          image: quay.io/openshift/origin-cli:latest
          imagePullPolicy: IfNotPresent
          resources:
            requests:
              memory: "64Mi"
              cpu: "150m"
            limits:
              memory: "256Mi"
              cpu: "200m"
          command:
            - /bin/sh
            - -c
            - |
              set -x

              export WAIOPS_NAMESPACE={{ WAIOPS_NAMESPACE }}
              export KAFKA_TOPIC_EVENTS=$(oc get kafkatopics -n $WAIOPS_NAMESPACE | grep -v cp4waiopscp4waiops|grep -v noi-integration | grep cp4waiops-cartridge-alerts-$EVENTS_TYPE| awk '{print $1;}')
              export KAFKA_TOPIC_LOGS=$(oc get kafkatopics -n $WAIOPS_NAMESPACE  | grep -v cp4waiopscp4waiops| grep cp4waiops-cartridge-logs-elk| awk '{print $1;}')
              export KAFKA_SECRET=$(oc get secret -n {{ WAIOPS_NAMESPACE }} |grep 'aiops-kafka-secret'|awk '{print$1}')
              export KAFKA_USER=$(oc get secret $KAFKA_SECRET -n $WAIOPS_NAMESPACE -o jsonpath='{.data.username}' | base64 --decode)
              export KAFKA_PWD=$(oc get secret $KAFKA_SECRET -n $WAIOPS_NAMESPACE -o jsonpath='{.data.password}' | base64 --decode)
              export KAFKA_BROKER=$(oc get routes iaf-system-kafka-0 -n $WAIOPS_NAMESPACE -o=jsonpath='{.status.ingress[0].host}{"\n"}'):443
              export CERT_ELEMENT=$(oc get secret -n $WAIOPS_NAMESPACE kafka-secrets  -o=jsonpath='{.data.ca\.crt}'| base64 -d| tr -d '\r\n')
              export LOG_ITERATIONS="20"
              export TOKEN="P4ssw0rd!"
              export USER_PASS="$(oc get secret -n $WAIOPS_NAMESPACE aiops-ir-core-ncodl-api-secret -o jsonpath='{.data.username}' | base64 --decode):$(oc get secret -n $WAIOPS_NAMESPACE aiops-ir-core-ncodl-api-secret -o jsonpath='{.data.password}' | base64 --decode)"
              export DATALAYER_ROUTE=$(oc get route  -n $WAIOPS_NAMESPACE datalayer-api  -o jsonpath='{.status.ingress[0].host}')
              export LOG_TIME_FORMAT="yyyy-mm-dd'T'HH:MM:ss.000000+00:00"
              #Increment between log lines in seconds
              export LOG_TIME_STEPS="3"
              # Advance clock by (seconds)
              export LOG_TIME_SKEW="0"
              # Time Zone
              export LOG_TIME_ZONE="0"

              echo "KAFKA_BROKER:         '"$KAFKA_BROKER"'"
              echo "KAFKA_USER:           '"$KAFKA_USER"'"
              echo "KAFKA_PWD:            '"$KAFKA_PWD"'"
              echo "KAFKA_TOPIC_EVENTS:   '"$KAFKA_TOPIC_EVENTS"'"
              echo "KAFKA_TOPIC_LOGS:     '"$KAFKA_TOPIC_LOGS"'"
              echo "CERT_ELEMENT:  |- "
              echo $CERT_ELEMENT
              echo "LOG_ITERATIONS:       '"$LOG_ITERATIONS"'"
              echo "TOKEN:                '"$TOKEN"'"
              echo "LOG_TIME_FORMAT:      '"$LOG_TIME_FORMAT"'"
              echo "LOG_TIME_STEPS:       '"$LOG_TIME_STEPS"'"
              echo "LOG_TIME_SKEW:        '"$LOG_TIME_SKEW"'"
              echo "LOG_TIME_ZONE:        '"$LOG_TIME_ZONE"'"



              cat <<EOF | oc apply -n {{ WAIOPS_NAMESPACE }} -f -
                apiVersion: v1
                kind: ConfigMap
                metadata:
                  name:  demo-ui-config
                  namespace: {{ WAIOPS_NAMESPACE }}
                data:
                  LOG_ITERATIONS: "$LOG_ITERATIONS"
                  TOKEN: $TOKEN
                  KAFKA_BROKER: $KAFKA_BROKER
                  KAFKA_USER: $KAFKA_USER
                  KAFKA_PWD: $KAFKA_PWD
                  KAFKA_TOPIC_EVENTS: $KAFKA_TOPIC_EVENTS
                  KAFKA_TOPIC_LOGS: $KAFKA_TOPIC_LOGS
                  DATALAYER_ROUTE: $DATALAYER_ROUTE
                  USER_PASS: $USER_PASS
                  LOG_TIME_FORMAT: "$LOG_TIME_FORMAT"
                  LOG_TIME_STEPS: "$LOG_TIME_STEPS"
                  LOG_TIME_SKEW: "$LOG_TIME_SKEW"
                  LOG_TIME_ZONE: "$LOG_TIME_ZONE"
                  CERT_ELEMENT:  |-
                    $CERT_ELEMENT
                  DEMO_EVENTS:  |-
                    {"events": [{"Node" : "mysql-github","NodeAlias" : "github","Manager" : "Github","AlertGroup" : "robot-shop","Severity" : 2,"Summary" : "[Git] Commit in repository robot-shop by Niklaus Hirt on file robot-shop.yaml - New Memory Limits","URL": "https://pirsoscom.github.io/git-commit-robot.html"},{"Node" : "mysql-instana","NodeAlias" : "mysql","Manager" : "InstanaInception","AlertGroup" : "robot-shop","Severity" : 3,"Summary" : "[Instana] MySQL - change detected - The value **resources/limits** has changed","URL": "https://pirsoscom.github.io/INSTANA_CHANGE_ROB.html"},{"Node" : "mysql-security","NodeAlias" : "mysql","Manager" : "Security","AlertGroup" : "robot-shop","Severity" : 2,"Summary" : "[Security] MySQL K8s Pod Created","URL": "none"},{"Node" : "mysql-predictive","NodeAlias" : "web","Manager" : "Metric Anomaly","AlertGroup" : "robot-shop","Severity" : 4,"Summary" : "[Predictive] MySQL memory peak usage anomaly","URL": "https://pirsoscom.github.io/METRICS_ROBOT_MEM_DETAIL.html"},{"Node" : "mysql-turbonomic","NodeAlias" : "mysql","Manager" : "Turbonomic","AlertGroup" : "robot-shop","Severity" : 4,"Summary" : "[Turbonomic] Container Resize - Controller Resize - Resize UP VMem Limit from 50 MB to 328 MB in Container Spec mysql","URL": "https://pirsoscom.github.io/TURBO_MYSQL_ACTIONS.html"},{"Node" : "mysql-turbonomic","NodeAlias" : "mysql","Manager" : "Turbonomic","AlertGroup" : "robot-shop","Severity" : 4,"Summary" : "[Turbonomic] Container Resize - Controller Resize - Resize UP VMem Limit from 50 MB to 328 MB in Container Spec mysql","URL": "https://pirsoscom.github.io/TURBO_MYSQL_ACTIONS.html"},{"Node" : "mysql-turbonomic","NodeAlias" : "mysql","Manager" : "Turbonomic","AlertGroup" : "robot-shop","Severity" : 4,"Summary" : "[Turbonomic] Container Resize - Controller Resize - Resize UP VMem Limit from 50 MB to 328 MB in Container Spec mysql","URL": "https://pirsoscom.github.io/TURBO_MYSQL_ACTIONS.html"},{"Node" : "mysql-turbonomic","NodeAlias"